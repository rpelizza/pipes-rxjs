{
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": true,
	"printWidth": 120,
	"arrowParens": "always",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"semi": true,
	"trailingComma": "es5",
	"bracketSameLine": true,
	"endOfLine": "crlf"
}
